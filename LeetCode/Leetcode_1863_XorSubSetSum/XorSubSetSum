package leetcode;


public class LeetCode_1863 {
	public int subsetXORSum(int[] nums) {
		return xor(nums, 0, 0);
	}
//

	private int xor(int[] nums, int cur, int i) {
		if (cur == nums.length)
			return i;

		int selected = xor(nums, cur + 1, i ^ nums[cur]);
		int nselected = xor(nums, cur + 1, i);

		return selected + nselected;
	}
}
